    "db:migrate-deploy:prod": "env-cmd -f .env.production npx prisma migrate deploy",
    "db:reset": "env-cmd -f .env.dev npx prisma migrate reset",
    "db:reset:preview": "env-cmd -f .env.preview npx prisma migrate reset",
    "postinstall": "patch-package",
    "prepare": "cd ../ && husky install ./web/.husky",
    "start-infra": "docker-compose -f infrastructure.yml up --detach",
    "vercel-build": "echo hannes && prisma generate && yarn db:migrate-deploy && next build",
    "insert-mock": "ts-node cli/db.ts 'createLaunchpadMockdata'",
    "insert-mock:dev": "env-cmd -f .env.dev ts-node cli/db.ts 'createLaunchpadMockdata'"
  },
  "husky": {
    "pre-commit": "npx tsc -p ./ --noEmit --incremental false"
