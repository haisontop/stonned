    "seed:prod": "env-cmd -f .env.production ts-node cli/db.ts",
    "build-check": "prisma generate && npx tsc -p ./ --noEmit --incremental false",
    "db:push": "env-cmd -f .env.dev npx prisma db push",
    "db:migrate": "env-cmd -f .env.dev npx prisma migrate deploy && env-cmd -f .env.dev npx prisma migrate dev --name",
    "db:migrate-deploy": "npx prisma migrate deploy",
    "db:migrate-deploy:dev": "env-cmd -f .env.dev npx prisma migrate deploy",
    "db:migrate-deploy:preview": "env-cmd -f .env.preview npx prisma migrate deploy",
