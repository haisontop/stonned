    "dev:staging": "env-cmd -f .env.staging next",
    "dev:prod": "env-cmd -f .env.production next",
    "build": "next build",
    "build:dev": "env-cmd -f .env.dev next build",
    "start": "next start",
    "cli": "env-cmd -f .env.dev ts-node cli/cli.ts",
    "cli:prod": "env-cmd -f .env.production ts-node cli/cli.ts",
